/* Actors */
cnt : std.CountTimer()
str : std.Stringify()
void : std.Void()
client : usr.SocketClient(address="127.0.0.1", port=12345, protocol="raw", type_="UDP")
sink : io.StandardOut(store_tokens=true)
dummysink : io.StandardOut(store_tokens=true)

/* Connections */
// Data
cnt.integer > str.in
str.out > client.inData
client.outData > sink.token

// Control
void.void > client.inControl
client.outControl > dummysink.token
